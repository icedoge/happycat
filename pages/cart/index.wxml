<block><view class="page-cart"><block><block wx:if="{{anonymousState__temp}}"><at-activity-indicator mode="center" __triggerObserer="{{ _triggerObserer }}"></at-activity-indicator></block><block wx:else><view><cm-cart cart="{{cart}}" __triggerObserer="{{ _triggerObserer }}"></cm-cart><cm-padding-height height="{{50}}" __triggerObserer="{{ _triggerObserer }}" safeAreaAdaptive="auto"></cm-padding-height><block><block wx:if="{{displayMode === ECartDisplayMode.View}}"><view class="{{footerClassName}}"><view class="panel-left"><view class="cart-footer-total"><text>合计:</text><cm-price value="{{cart.info.payableAmount}}" __triggerObserer="{{ _triggerObserer }}" camelize="{{false}}"></cm-price></view><view class="cart-footer-detail"><text>总额:</text><cm-price value="{{cart.info.totalAmount}}" __triggerObserer="{{ _triggerObserer }}" camelize="{{false}}"></cm-price><text>优惠:</text><cm-price value="{{cart.info.discountAmount}}" __triggerObserer="{{ _triggerObserer }}" camelize="{{false}}"></cm-price><text>(不包含运费)</text></view></view><view class="panel-right"><at-button className="checkout-button" __triggerObserer="{{ _triggerObserer }}" type="primary" full="{{true}}" bindonclick="onCheckout" __fn_onClick="{{true}}">去结算 ({{cart.info.totalQuantity}})</at-button></view></view></block><block wx:else><view class="{{footerClassName}}"><view class="panel-left"></view><view class="panel-right"><at-button className="delete-button" __triggerObserer="{{ _triggerObserer }}" type="secondary" circle="{{true}}" size="small" bindonclick="onDeleteSelectedBasketItem" __fn_onClick="{{true}}">删除选中的商品</at-button></view></view></block></block></view></block></block></view></block>